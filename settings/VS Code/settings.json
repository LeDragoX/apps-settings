{
  // cmake
  "cmake.pinnedCommands": ["workbench.action.tasks.configureTaskRunner", "workbench.action.tasks.runTask"],
  "cmake.showConfigureWithDebuggerNotification": false,
  "cmake.showOptionsMovedNotification": false,
  // javascript
  "javascript.format.semicolons": "remove",
  "javascript.updateImportsOnFileMove.enabled": "always",
  // typescript
  "typescript.format.semicolons": "insert",
  "typescript.suggest.completeFunctionCalls": true,
  // cSpell
  "cSpell.enableFiletypes": ["powershell"],
  "cSpell.language": "en,pt",
  "cSpell.userWords": ["Fullscreen", "immediates", "Immediates", "Structurize"],
  // discord
  "discord.detailsIdling": "üîé Searching on Files...",
  "discord.detailsEditing": "üöÄ {file_name} with {file_size}",
  "discord.largeImageIdling": "üë®‚Äçüíª Coding and Relaxing",
  "discord.lowerDetailsDebugging": "üî® Debugging: {git_branch}",
  "discord.lowerDetailsEditing": "üìÅ Project: {workspace}",
  "discord.lowerDetailsIdling": "üï° Refreshing the mind",
  "discord.lowerDetailsNoWorkspaceFound": "{file_size}",
  // editor
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": true,
  "editor.inlineSuggest.enabled": true,
  "editor.fontFamily": "'JetBrains Mono', 'MesloLGS NF', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "editor.rulers": [50, 100, 150, 200],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell",
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.trimTrailingWhitespace": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.tabSize": 2
  },
  // explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".prettierrc": "json"
  },
  // git
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.enableCommitSigning": true,
  // github copilot
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  // gitlens
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultDateLocale": "pt-BR",
  "gitlens.defaultDateFormat": "DD-MM-YYYY H:MM a",
  "gitlens.defaultTimeFormat": "H:mm a",
  "gitlens.defaultDateShortFormat": null,
  // githubPullRequests
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  // markdown-preview-enhanced
  "markdown-preview-enhanced.previewTheme": "monokai.css",
  // prettier
  "prettier.arrowParens": "avoid",
  "prettier.printWidth": 140,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  // tabnine
  "tabnine.experimentalAutoImports": true,
  // terminal
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontSize": 14,
  // workbench
  "workbench.colorTheme": "One Monokai",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.lnk": "default",
    "*.sqlite3": "default"
  },
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  // misc
  "console-ninja.featureSet": "Pro",
  "diffEditor.ignoreTrimWhitespace": false,
  "prettier.endOfLine": "auto",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "powershell.codeFormatting.preset": "OTBS",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "yaml.schemas": {
    "file:///c%3A/Users/plini/.vscode/extensions/docsmsft.docs-yaml-1.0.4/dist/toc.schema.json": "/toc\\.yml/i"
  }
}
